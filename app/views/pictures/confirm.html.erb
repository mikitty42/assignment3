<%= form_with(model: @picture,local: true) do |form| %>
    
    <%= image_tag(@pictute.image.url) if @pictute.image && @pictute.image.url %>
    <%= form.hidden_field :content %>
    <%= form.hidden_field :image_cache %>
    
    <%= form.submit '登録する' %>
<% end %>

<%= form_with(model: @pictute,local: true) do |form| %>
    <%= form.hidden_field :image_cache %>

    <%= form.submit '戻る',name: 'back' %>
<% end %>
